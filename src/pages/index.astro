---
import BaseHead from '../components/BaseHead.astro';
import { Image } from 'astro:assets';
import mainPageImage from '../images/IMG_2232.JPG';
import { SITE_TITLE, SITE_DESCRIPTION } from '../consts';
---

<!doctype html>
<html lang="en">
	<head>
		<BaseHead title={SITE_TITLE} description={SITE_DESCRIPTION} />
	</head>
	<body>
		<main>
			<p>
				<Image src={mainPageImage} alt="StrongEra Studio coming soon" style='width: 500; object-fit: contain'/>
			</p>
		</main>
	</body>
</html>
